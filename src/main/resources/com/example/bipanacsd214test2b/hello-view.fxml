<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="461.0" prefWidth="431.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bipanacsd214test2b.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="iD" promptText="ReservationID" />
   <TextField fx:id="Cus" promptText="CustomerName" />
   <TextField fx:id="date" promptText="ReservationDate" />
   <TextField fx:id="NoG" promptText="NumOfGuests" />
   <Button mnemonicParsing="false" onAction="#UpdateData" text="Update" />
   <Button mnemonicParsing="false" onAction="#InsertNew" text="Insert" />
   <Button mnemonicParsing="false" onAction="#Deletedata" text="Delete" />
   <Button mnemonicParsing="false" onAction="#LoadData" text="Load" />
    <Button onAction="#onHelloButtonClick" text="Click" />

    <Label fx:id="welcomeText" />
    <TableView fx:id="reservation" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn fx:id="ReservationID" prefWidth="75.0" text="ReservationID" />
            <TableColumn fx:id="CustomerName" prefWidth="75.0" text="CustomerName" />
            <TableColumn fx:id="ReservationDate" prefWidth="75.0" text="ReservationDate" />
            <TableColumn fx:id="NumOfGuests" prefWidth="75.0" text="NumOfGuests" />
        </columns>
    </TableView>
</VBox>
